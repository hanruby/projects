package com.eatifying.commons.domain.user;

import com.eatifying.commons.domain.GeoCode;

public class UserAddress {
	int cutomerAddressId;
	String addressLine1;
	String addressLine2;
	String place;
	String city;
	int zipcode;
	String directions;
	float latitude;
	float longitude;
	GeoCode geoCode;

	public int getCutomerAddressId() {
		return cutomerAddressId;
	}

	public void setCutomerAddressId(int cutomerAddressId) {
		this.cutomerAddressId = cutomerAddressId;
	}

	public String getAddressLine1() {
		return addressLine1;
	}

	public void setAddressLine1(String addressLine1) {
		this.addressLine1 = addressLine1;
	}

	public String getAddressLine2() {
		return addressLine2;
	}

	public void setAddressLine2(String addressLine2) {
		this.addressLine2 = addressLine2;
	}

	public String getPlace() {
		return place;
	}

	public void setPlace(String place) {
		this.place = place;
	}

	public String getCity() {
		return city;
	}

	public void setCity(String city) {
		this.city = city;
	}

	public int getZipcode() {
		return zipcode;
	}

	public void setZipcode(int zipcode) {
		this.zipcode = zipcode;
	}

	public String getDirections() {
		return directions;
	}

	public void setDirections(String directions) {
		this.directions = directions;
	}

	public float getLatitude() {
		return latitude;
	}

	public void setLatitude(float latitude) {
		this.latitude = latitude;
	}

	public float getLongitude() {
		return longitude;
	}

	public void setLongitude(float longitude) {
		this.longitude = longitude;
	}

	public GeoCode getGeoCode() {
		return geoCode;
	}

	public void setGeoCode(GeoCode geoCode) {
		this.geoCode = new GeoCode();
		this.geoCode.latitude = this.latitude;
		this.geoCode.longitude = this.longitude;
	}
}
