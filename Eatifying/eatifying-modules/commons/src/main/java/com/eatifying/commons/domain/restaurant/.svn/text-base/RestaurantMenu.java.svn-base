package com.eatifying.commons.domain.restaurant;

import java.util.Date;
import java.util.HashMap;
import java.util.List;
import java.util.Map;

public class RestaurantMenu {
	private int restaurantId;

	private int restaurantMenuId;

	private Date addedTime;

	private Date updatedTime;

	private List<RestaurantMenuItem> menuItems;

	private Map<RestaurantMenuItemCategory, List<RestaurantMenuItem>> categoryMenuMap 
			= new HashMap<RestaurantMenuItemCategory, List<RestaurantMenuItem>>();

	public int getRestaurantId() {
		return restaurantId;
	}

	public void setRestaurantId(int restaurantId) {
		this.restaurantId = restaurantId;
	}

	public int getRestaurantMenuId() {
		return restaurantMenuId;
	}

	public void setRestaurantMenuId(int restaurantMenuId) {
		this.restaurantMenuId = restaurantMenuId;
	}

	public Date getAddedTime() {
		return addedTime;
	}

	public void setAddedTime(Date addedTime) {
		this.addedTime = addedTime;
	}

	public Date getUpdatedTime() {
		return updatedTime;
	}

	public void setUpdatedTime(Date updatedTime) {
		this.updatedTime = updatedTime;
	}

	public List<RestaurantMenuItem> getMenuItems() {
		return menuItems;
	}

	public void setMenuItems(List<RestaurantMenuItem> menuItems) {
		this.menuItems = menuItems;
	}

	public Map<RestaurantMenuItemCategory, List<RestaurantMenuItem>> getCategoryMenuMap() {
		return categoryMenuMap;
	}

	public void setCategoryMenuMap(
			Map<RestaurantMenuItemCategory, List<RestaurantMenuItem>> categoryMenuMap) {
		this.categoryMenuMap = categoryMenuMap;
	}

}
