<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.eatifying.dao.EmployeeMapper">

	<cache />
	<select id="getEmployeeList" resultType="com.eatifying.commons.domain.Employee">
		SELECT
		employeeID,
		firstName,
		lastName,
		email
		FROM Employee
  </select>

</mapper>