	margin: 0 auto;
	padding-top: 15px;
}

.header_cont h1 {
	float: left;
}

.header_cont h1 a {
	background: Feldspar no-repeat;
	color: #000;
	display: block;
	font-family: 'Merienda One', cursive;
	font-size: xx-large;
	text-shadow: rgba(17, 0, 0, 0.5) 0 7px 7px;
}

.header_cont h1 a:hover {
