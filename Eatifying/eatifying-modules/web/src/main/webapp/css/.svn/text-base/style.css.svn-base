@charset "utf-8";
/* CSS Document */
@import url(http://fonts.googleapis.com/css?family=Merienda+One);

@import url("reset.css");
/*  Reset for some default browser CSS - General */
body,div,h1,h2,h3,p,ul,ol,li,img,header,section,aside,footer,button {
	margin: 0;
	padding: 0;
	border: 0;
	list-style: none;
}

html,body {
	height: 100%;
}

a {
	text-decoration: none;
	color: #FFF;
	/*color: #FD3E4E;*/
}

a:hover {
	/*color: #6f92a7;*/
}

a:focus {
	outline: 0;
}

h1,h2,h3 {
	font-weight: normal;
}

h2 {
	font-size: 22px;
	line-height: 28px;
	color: #9CAD0F;
}

h3 {
	font-size: 18px;
	line-height: 24px;
}

.float {
	float: left;
}

body {
	background: #E5E5D8;
	font: normal 13px/100% Verdana, Tahoma, sans-serif;
	font-size: 14px;
	margin: 0 auto;
	/*background: #EDCD78 url('../images/grey-bg.gif');*/
	background:#f8f8f8 url('../images/body_bg.gif') repeat;
	/*width: 960px;*/
}
/* Header */
header#site_head {
	/*background-color: rgba(163, 2, 2, 0.65);
	*/
	-webkit-box-shadow: inset 0 -5px 8px -5px rgba(0, 0, 0, 0.65);
	-moz-box-shadow: inset 0 -5px 8px -5px rgba(0, 0, 0, 0.65);
	box-shadow: inset 0 -5px 8px -5px rgba(0, 0, 0, 0.65);
	height: 60px;
	
	/*
	background: Background;
	-moz-box-shadow: 10px 10px 5px #888;
	-webkit-box-shadow: 10px 10px 5px #888;
	box-shadow: 10px 10px 5px #888;
	box-shadow: inset 0px -2px 3px #480508;
	border-bottom: 1px solid #fffffb;
	height: 60px;
	/*position: fixed;*/
	top: 0;
	left: 0;
	right: 0;
	overflow: visible;
	width: 100%;
	/* I tried width: 100% but it does not seem to work on the header */
	margin: 0 auto;
}

.header_cont { /*width: 960px;*/
